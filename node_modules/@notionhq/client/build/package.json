{
    "name": "@notionhq/client",
    "version": "0.4.4",
    "description": "A simple and easy to use client for the Notion API",
    "engines": {
        "node": ">=12"
    },
    "homepage": "https://developers.notion.com/docs/getting-started",
    "bugs": {
        "url": "https://github.com/makenotion/notion-sdk-js/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/makenotion/notion-sdk-js/"
    },
    "keywords": [
        "notion",
        "notionapi",
        "rest",
        "notion-api"
    ],
    "main": "./build/src",
    "scripts": {
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint && npm run test",
        "build": "tsc",
        "prettier": "prettier --write .",
        "lint": "prettier --check . && eslint . --ext .ts && cspell '**/*' ",
        "test": "ava",
        "check-links": "git ls-files | grep md$ | xargs -n 1 markdown-link-check",
        "prebuild": "npm run clean",
        "clean": "rm -rf ./build"
    },
    "author": "",
    "license": "MIT",
    "files": [
        "build/package.json",
        "build/src/**"
    ],
    "dependencies": {
        "@types/node-fetch": "^2.5.10",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@ava/typescript": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "ava": "^3.15.0",
        "cspell": "^5.4.1",
        "eslint": "^7.24.0",
        "markdown-link-check": "^3.8.7",
        "prettier": "^2.3.0",
        "typescript": "^4.2.4"
    }
}
